{
  "compilerOptions": {
    "target": "ES2024",
    "module": "Node16",
    "rootDir": "./src",
    "moduleResolution": "node16",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}